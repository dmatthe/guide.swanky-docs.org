{
  "name": "docs",
  "description": "Swanky Documentation",
  "scripts": {
    "start": "NODE_ENV=development node docs/config/start/serve.dev.js",
    "prebuild": "rimraf build/",
    "build": "npm run build:prod",
    "build:serve": "npm-run-all build serve:prod",
    "build:prod": "NODE_ENV=production webpack -p --progress --config docs/config/build/build.prod.js --profile --colors",
    "serve:prod": "http-server build -o"
  },
  "dependencies": {
    "prismjs": "1.5.0"
  },
  "devDependencies": {
    "http-server": "0.9.0",
    "npm-run-all": "2.1.2",
    "rimraf": "2.5.2",
    "swanky-server": "2.1.0",
    "webpack": "2.1.0-beta.21"
  }
}
